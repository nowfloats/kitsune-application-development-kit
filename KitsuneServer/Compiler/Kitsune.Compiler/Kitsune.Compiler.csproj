<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>1.0.1</Version>
    <Authors>Devesh Kumar</Authors>
    <Company>Kitsune</Company>
    <Copyright>Copyright Â©  2021</Copyright>
    <OutputType>Library</OutputType>
    <RootNamespace>Kitsune.Compiler</RootNamespace>
    <AssemblyName>Kitsune.Compiler</AssemblyName>
    <CheckForOverflowUnderflow>true</CheckForOverflowUnderflow>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <FileAlignment>512</FileAlignment>
    <SccProjectName>SAK</SccProjectName>
    <SccLocalPath>SAK</SccLocalPath>
    <SccAuxPath>SAK</SccAuxPath>
    <SccProvider>SAK</SccProvider>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <LangVersion>default</LangVersion>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug\</OutputPath>
    <DocumentationFile>bin\Debug\Kitsune.Compiler.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/nowfloats/kitsune-application-development-kit</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.36" />
    <PackageReference Include="MongoDB.Driver" Version="2.13.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="NJsonSchema" Version="9.10.27" />
    <PackageReference Include="protobuf" Version="2.6.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Common\Kitsune.Helper\Kitsune.Helper.csproj" />
    <ProjectReference Include="..\..\Common\Kitsune.Language\Kitsune.Language.csproj" />
    <ProjectReference Include="..\..\Common\Kitsune.Models\Kitsune.Models.csproj" />
    <ProjectReference Include="..\..\Common\Kitsune.SyntaxParser\Kitsune.SyntaxParser.csproj" />
    <ProjectReference Include="..\..\Kitsune.API.Model\Kitsune.API.Model.csproj" />
  </ItemGroup>

</Project>
